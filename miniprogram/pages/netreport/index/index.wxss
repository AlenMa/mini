/* miniprogram/pages/netreport/index/index.wxss */


.btn {
    width: 100%;
    height: 0rpx;
    margin: 0;
    padding: 0;
    border-radius: 0;
    border: 0;
    position: absolute;
    top: 0;
    overflow: inherit;
    margin-bottom: 40px;
}

.btnform::after {
  border: none;
}

.btnform {
  border-radius:0;
  background-color:#fff;
  border: 0rpx;
  margin: 5rpx;
  padding: 5rpx;
  height: 100%;
  display: flex;
  align-items:center;
  line-height:1;
  font-size: 32rpx;
}

.submitbtn{
  margin-top: 20rpx;
  width: 80%;
  display: flex;
  justify-content: center;
}